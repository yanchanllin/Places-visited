<%= form_for(@place) do |place_builder| %>
    <%= render 'shared/error_messages' %>
    <p><strong>Place Visited</strong> 
      <%= place_builder.label :title %>
      <%= place_builder.text_field :title, class: 'form-control' %><br>
    </p>
  
    <%= place_builder.submit "Add Place", class: "btn btn-md btn-primary" %>
<% end %>